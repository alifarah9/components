{
  "name": "@transferwise/components",
  "version": "8.1.0",
  "description": "TransferWise styleguide components in react",
  "license": "MIT",
  "main": "./build/main.js",
  "repository": {
    "fullname": "transferwise/components",
    "type": "git",
    "url": "git+https://github.com/transferwise/components.git"
  },
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-dynamic-import-node": "1.1.0",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-destructuring": "6.23.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-react-jsx-self": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "bootstrap": "github:transferwise/bootstrap",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.1",
    "currency-flags": "^1.2.1",
    "deploy-directory-on-branch-to-gh-pages": "^0.2.4",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "fs-extra": "0.30.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^0.14.3",
    "jest": "21.2.1",
    "jest-cli": "^21.2.1",
    "jest-fetch-mock": "^2.1.1",
    "json-loader": "0.5.4",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "lint-staged": "^6.0.1",
    "lodash.times": "^4.3.2",
    "object-assign": "4.1.1",
    "postcss-loader": "1.2.2",
    "prettier-eslint-cli": "^4.7.0",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-dev-utils": "^0.5.2",
    "react-dom": "^16.3.2",
    "react-test-renderer": "^16.3.2",
    "release-to-github-with-changelog": "^1.1.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0"
  },
  "dependencies": {
    "@transferwise/formatting": "^1.4.3",
    "@transferwise/icons": "^0.14.0",
    "@transferwise/neptune-css": "^0.3.0",
    "classnames": "^2.2.6",
    "react-transition-group": "^2.3.1"
  },
  "peerDependencies": {
    "bootstrap": "github:transferwise/bootstrap",
    "react-dom": "^16.3.2",
    "currency-flags": "^1.2.1",
    "react": "^16.3.2",
    "prop-types": "^15.6.1"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:watch": "jest --watch --env=jsdom",
    "pretest": "npm run check-format && npm run check-release-format",
    "check-release-format": "node_modules/.bin/release-to-github-with-changelog-pre-release-checks",
    "check-format": "prettier-eslint '**/*.js' --list-different",
    "release-to-github": "release-to-github-with-changelog",
    "predeploy": "npm run build",
    "build-docs": "node scripts/build.js docs",
    "deploy-docs": "gh-pages -a -d docs-build",
    "deploy-docs-for-branch": "deploy-directory-on-branch-to-gh-pages -d docs-build"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-filename-extension": 0,
      "no-mixed-operators": 0
    },
    "env": {
      "browser": true,
      "jest": true
    }
  },
  "homepage": "https://transferwise.github.io/components/",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint '**/*.js' --write",
      "git add"
    ]
  }
}
